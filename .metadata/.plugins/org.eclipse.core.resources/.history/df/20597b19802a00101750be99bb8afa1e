package pkg1;
import java.io.File;
import java.io.IOException;
import java.util.Scanner;
import java.util.ArrayList;
import java.util.List;
public class Test11 {

	public static void main(String[] args) throws IOException{
		File f1=new File("amount.txt");
		Scanner sc1=new Scanner(f1);
		List<String> list1=new ArrayList<>();
		String s1;
		String word1;
		for(int i=0;i<=21;i++) {
			s1=sc1.nextLine();
			list1.add(s1);
			
		}
		System.out.println(list1);
		for(int i=0;i<=20;i++) {
			System.out.println(Integer.toString(i) + " - " +list1.get(i));
		}
		for(int i=1;i<10;i++) {
			word1=list1.get(20)+" "+list1.get(i);
			System.out.println(Integer.toString(20+i) + " - "+ word1);
		}
		System.out.println(Integer.toString(30+0) + " - "+list1.get(21));
		for(int i=1;i<10;i++) {
			word1=list1.get(21)+" "+list1.get(i);
			System.out.println(Integer.toString(30+i) + " - "+ word1);
		}
		System.out.println(Integer.toString(40+0) + " - "+list1.get(22));
		

	}

}
